{
  "name": "electron-releases",
  "description": "Complete and up-to-date info about every release of Electron",
  "repository": "https://github.com/electron/electron-releases",
  "version": "3.678.0",
  "exports": "./index.json",
  "type": "module",
  "license": "MIT",
  "devDependencies": {
    "@octokit/rest": "^18.0.7",
    "chai": "^4.2.0",
    "check-for-leaks": "^1.2.1",
    "dotenv-safe": "^8.2.0",
    "flat": "^5.0.2",
    "github-slugger": "^1.3.0",
    "got": "^11.8.0",
    "hubdown": "^2.3.2",
    "json-to-markdown-table": "^1.0.0",
    "make-promises-safe": "^5.1.0",
    "mdast-util-to-string": "^3.1.0",
    "mocha": "^8.2.0",
    "parse-link-header": "^1.0.1",
    "platform-utils": "^1.2.0",
    "prettier": "^2.1.2",
    "semver": "^7.3.2",
    "unist-util-visit": "^3.0.1"
  },
  "scripts": {
    "build": "npm run collect && npm run lite && npm run update-readme",
    "collect": "node script/collect.mjs --experimental-json-modules",
    "lite": "node script/lite.js --experimental-json-modules > lite.json",
    "update-readme": "node script/update-readme.mjs --experimental-json-modules",
    "prettier": "prettier --check \"./**/*.mjs\"",
    "test": "mocha --timeout 5000 --experimental-json-modules && prettier --check \"./**/*.mjs\" --write",
    "prepack": "check-for-leaks",
    "prepush": "check-for-leaks",
    "release": "./script/release.sh"
  }
}
